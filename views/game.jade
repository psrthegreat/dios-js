doctype html
html(lang='en')
	head
		meta(charset='UTF-8')
		title Game
		script(type='text/javascript', src='/ethan/phaser.min.js')
		script(type='text/javascript', src='/vendor/jquery.js')
		script(type='text/javascript', src='/vendor/bootstrap.min.js')
		link(rel="stylesheet", href='/vendor/bootstrap.min.css')
		script(type='text/javascript', src='/ethan/person.js')
		script(type='text/javascript', src='/ethan/people.js')
		script(type='text/javascript', src='/ethan/index.js')
		link(rel="stylesheet", href='/ethan/game.css')
	body
		section
			.container
				.row
					.col-md-7
						#phaser
					.col-md-4
						form#mainF(action="/generate", method="get")
							input.hidden#dat(type="text", name="q")
							button.btn.col-lg-12#submit(type="submit")
								| Submit
						pre#js(contenteditable=true)
							!{JSON.stringify(data, null, 2)}
		script.
			var data =!{JSON.stringify(data)};
		script(type='text/javascript', src='/ethan/index.js')
		script.
			$('#mainF').submit(function(){
				$('#dat').val(JSON.stringify(JSON.parse($('#js').text())));
			});